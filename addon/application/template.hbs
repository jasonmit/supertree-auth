{{#each flashMessages.queue as |flash|}}
  {{flash-message flash=flash}}
{{/each}}
{{#if session.isAuthenticated}}
  <header class="header">
    <div class="logo">{{t 'app.name'}}</div>
    {{layout/main-menu}}
  </header>
  <main>
    {{users/session-form}}
  </main>
  <footer></footer>
{{else}}
  {{outlet}}
{{/if}}
